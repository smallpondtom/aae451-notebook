  <flight_control>
      
    <function name="fcs/engine-roll-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.1
                100  0.1
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="fcs/engine-pitch-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.1
                100  0.1
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="fcs/aero-moment-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0
                100  0
                200 1
                300 1
            </tableData>
        </table>
    </function>

    <function name="fcs/engine-yaw-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.1
                100  0.1
                200 0
                300 0
            </tableData>
        </table>
    </function>

    <function name="fcs/lift-fan-thrust-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.9
                100  0.9
                200 0.0
                300 0.0
            </tableData>
        </table>
    </function>

    <function name="fcs/engine-thrust-mix">
        <table>
            <independentVar>velocities/vt-fps</independentVar>
            <tableData>
                0   0.9
                100  0.9
                200 1.0
                300 1.0
            </tableData>
        </table>
    </function>

    <channel name="Gear">
      <pure_gain>
        <input>gear/gear-cmd-norm</input>
        <gain>1</gain>
        <output>gear/gear-pos-norm</output>
      </pure_gain>
    </channel>

    <channel name="Elevator">
      <pure_gain>
        <input>fcs/elevator-cmd-norm</input>
        <gain>fcs/aero-moment-mix</gain>
        <output>fcs/elevator-pos-rad</output>
      </pure_gain>
    </channel>

    <channel name="Left Aileron">
      <pure_gain>
        <input>fcs/aileron-cmd-norm</input>
        <gain>-fcs/aero-moment-mix</gain>
        <output>fcs/left-aileron-pos-rad</output>
      </pure_gain>
    </channel>

    <channel name="Right Aileron">
      <pure_gain>
        <input>fcs/aileron-cmd-norm</input>
        <gain>fcs/aero-moment-mix</gain>
        <output>fcs/right-aileron-pos-rad</output>
      </pure_gain>
    </channel>

    <channel name="Rudder">
      <pure_gain>
        <input>fcs/rudder-cmd-norm</input>
        <gain>fcs/aero-moment-mix</gain>
        <output>fcs/rudder-pos-rad</output>
      </pure_gain>
    </channel>
      
      
    <channel name="Engine0">
      <pure_gain name="fcs/elevator_to_engine0">
        <input>fcs/elevator-cmd-norm</input>
        <gain>fcs/engine-pitch-mix</gain>
      </pure_gain>
      <pure_gain name="fcs/throttle_to_engine0">
        <input>fcs/throttle-cmd-norm</input>
        <gain>fcs/engine-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>fcs/elevator_to_engine0</input>
        <input>fcs/throttle_to_engine0</input>
        <output>fcs/throttle-pos-norm</output>
      </summer>
    </channel>

    <channel name="Engine0_Yaw">
      <pure_gain name="fcs/rudder_to_engine0_yaw">
        <input>fcs/rudder-cmd-norm</input>
        <gain>fcs/engine-yaw-mix</gain>
        <output>propulsion/engine/yaw-angle-rad</output>
      </pure_gain>
    </channel>

    <channel name="Engine1">
      <pure_gain name="fcs/elevator_to_engine1">
        <input>fcs/elevator-cmd-norm</input>
        <gain>-fcs/engine-pitch-mix</gain>
      </pure_gain>
      <pure_gain name="fcs/throttle_to_engine1">
        <input>fcs/throttle-cmd-norm</input>
        <gain>fcs/lift-fan-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>fcs/elevator_to_engine1</input>
        <input>fcs/throttle_to_engine1</input>
        <output>fcs/throttle-pos-norm[1]</output>
      </summer>
    </channel>

    <channel name="Engine2">
      <pure_gain name="fcs/aileron_to_engine2">
        <input>fcs/aileron-cmd-norm</input>
        <gain>fcs/engine-roll-mix</gain>
      </pure_gain>
      <pure_gain name="fcs/throttle_to_engine2">
        <input>fcs/throttle-cmd-norm</input>
        <gain>fcs/lift-fan-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>fcs/aileron_to_engine2</input>
        <input>fcs/throttle_to_engine2</input>
        <output>fcs/throttle-pos-norm[2]</output>
      </summer>
    </channel>

    <channel name="Engine3">
      <pure_gain name="fcs/aileron_to_engine3">
        <input>fcs/aileron-cmd-norm</input>
        <gain>-fcs/engine-roll-mix</gain>
      </pure_gain>
      <pure_gain name="fcs/throttle_to_engine3">
        <input>fcs/throttle-cmd-norm</input>
        <gain>fcs/lift-fan-thrust-mix</gain>
      </pure_gain>
      <summer>
        <input>fcs/aileron_to_engine3</input>
        <input>fcs/throttle_to_engine3</input>
        <output>fcs/throttle-pos-norm[3]</output>
      </summer>
    </channel>


